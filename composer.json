{
    "name": "okvpn/migration-bundle",
    "type": "symfony-bundle",
    "description": "Database Schema migrations using Doctrine",
    "keywords": ["Migration", "Schema", "Data", "Fixtures"],
    "homepage": "https://github.com/okvpn/migration-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        },
        {
            "name": "Tsykun Vladimir",
            "email": "vtsykun@okvpn.org"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/framework-bundle": "~2.7|~3.0",
        "symfony/doctrine-bridge": "~2.7|~3.0",
        "symfony/twig-bundle": "~2.7|~3.0",
        "doctrine/data-fixtures": "^1.0",
        "doctrine/dbal": "^2.1",
        "doctrine/orm": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": { "Okvpn\\Bundle\\MigrationBundle\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
